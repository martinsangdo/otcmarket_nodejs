				<td><input type="text" class="symbol"/></td>
				<td><input type="button" value="X" onclick="removeLine(this);"/></td>
		</tr>
	</table>
	<input type="button" value="Add Option" onclick="addOption();"/>
	<div class="align_right">
		<input type="button" value="Export CSV" onclick="exportCSV();"/>
